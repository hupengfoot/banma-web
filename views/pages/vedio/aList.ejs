<div class="row aList">
    <% var imgUrl = "" %>
    <% var href = "" %>
    <% var dateTime = aRow.subject.createDate.split('T')[0].split('-')[0] + "-" + aRow.subject.createDate.split('T')[0].split('-')[1]%>
    <% if(aRow.subject.videoId === undefined || aRow.subject.videoId === "" || aRow.subject.videoId === null){ %>
    <% if(aRow.subject.image[0].url.substring(0, 4) === "http"){ %>
    <% imgUrl = aRow.subject.image[0].url %>
    <% }else{ %>
    <% imgUrl = "http://7xkufz.com2.z0.glb.qiniucdn.com/" + aRow.subject.image[0].url %>
    <% } %>
    <% href = "/vedio/subjectdetail?passport=&subjectId=" + aRow.subject.id %>
    <% }else{ %>
    <% imgUrl = "http://7xkufz.com2.z0.glb.qiniucdn.com/" + aRow.videoPoster %>
    <% href = "/vedio/detail?passport=&videoId=" + aRow.subject.videoId + "&objType=" + aRow.subject.objType + "&objId=" + aRow.subject.objId %>
    <% } %>
    <a target="_blank" href="<%= href %>">
    <div class="col-lg-2">
        <span> 
            <img  class="imgBox img-rounded" src="<%= imgUrl %>">
        </span>
    </div>
    <div class="col-lg-6">
        <div class="aTitle">
            <p style="font-family:Microsoft YaHei;font-size:14px;color:#000000;"><%= aRow.title %></p>
        </div>
        <div class="aDesc">
            <p style="font-family:Microsoft YaHei;font-size:14px;color:#666666;"><%= aRow.subject.summary %></p>
        </div>
        <div class="row">
	   <div class="col-lg-2">
		<img src="../img/qiutan-watch.png"></img>
		<span><%= aRow.subject.likeCount %></span>
	   </div>
	   <div class="col-lg-4">
		<img src="../img/qiutan-time.png"></img>
		<span><%= dateTime %></span>
	   </div>
        </div>
    </div>
    </a>
</div>

